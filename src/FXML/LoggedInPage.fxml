<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller">
    <center>
        <GridPane alignment="CENTER">
            <Button fx:id="logOutViewExcursions" text="View excursions" GridPane.rowIndex="0" GridPane.columnIndex="0" onAction="#viewExcursions"/>
            <Button fx:id="logOutManageBookings" text="Manage Bookings" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <Button fx:id="logOutLogOut" text="Log out" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#logOut"/>
        </GridPane>
    </center>
</BorderPane>
